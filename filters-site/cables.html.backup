    <!-- Get Quote Modal -->
    <div id="whatsappQuoteModal" class="modal">
      <div class="modal-content">
        <div class="modal-header">
          <h3>Request a Quote</h3>
          <span class="close" onclick="closeWhatsAppQuoteModal()">&times;</span>
        </div>
        <form id="quoteForm" onsubmit="submitQuote(event)">
          <div class="form-group">
            <label for="quoteName">Full Name *</label>
            <input type="text" id="quoteName" name="name" required />
          </div>

          <div class="form-group">
            <label for="quoteCompany">Company Name *</label>
            <input type="text" id="quoteCompany" name="company" required />
          </div>

          <div class="form-group">
            <label for="quoteMobile">Mobile Number *</label>
            <input type="tel" id="quoteMobile" name="mobile" required />
          </div>

          <div class="form-group">
            <label for="quoteFile">Attach File (Optional)</label>
            <input
              type="file"
              id="quoteFile"
              name="attachment"
              accept=".pdf,.doc,.docx,.jpg,.png"
            />
          </div>

          <div class="form-group">
            <label for="quoteDescription">Describe Your Requirements *</label>
            <textarea
              id="quoteDescription"
              name="description"
              rows="4"
              required
              placeholder="Please describe the cables or wiring products you need a quote for..."
            ></textarea>
          </div>

          <div class="form-actions">
            <button
              type="button"
              class="btn-outline"
              onclick="closeWhatsAppQuoteModal()"
            >
              Cancel
            </button>
            <button type="submit" class="btn-primary">
              <i class="fas fa-paper-plane"></i>
              Send Quote Request
            </button>
          </div>
        </form>
